{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "jsx": "react",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitAny": false
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  